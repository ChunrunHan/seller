/**app.wxss**/
 @import "style/weui.wxss"; 
 @import "style/font.wxss"; 
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
} 

page{
background-color:#efeff4;
}

.switch-commit {
    background: #F05141;
    color: white;
    font-size: 20px;
    border-radius: 8px;
    border: 1px solid #f4511e;
    width: 90%;
    margin-left: 5%;
    height: 40px;
    text-align: center;
    line-height: 35px;

}


.font-15{
  font-size: 15px;
}

.font-30{
  font-size: 30px;
}

.font-40{
  font-size: 40px;
}

.del-commit{
    background:white;
    color: #F05141;
}



.color-gray{
  color:#999999;
}

.color-auto{
    color: #F05141;
}

.app-line{
  background-color: #efeff4 !important;
  height: 20px;
}

.width100{
  width: 100%;
}

.mgt-0{
  margin-top: 0px !important;
}
.mgt-10{
  margin-top: 10px;
}

.mgl-10{
  margin-left: 10px;
}

.mgr-10{
  margin-right: 10px;
}

.mgt-20{
  margin-top: 20px !important;
}

.mgb-20{
  margin-bottom: 20px !important;
}

.text-center{
  text-align: center;
}
.text-left{
  text-align: left;
}
.text-right{
  text-align: right;
}
.bg-white{
  background: white;
}

.pull-lett{
