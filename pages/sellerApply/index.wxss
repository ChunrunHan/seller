.box {
    width: 100%;
    height: 160px;
    background: #f05141;
    text-align: center;
}

.img-logo {
    display: inline-block;
    width: 100px;
    height: 100px;
    /* border-radius: 50%; */
    margin-top: 15px;
}

.cancel{
  position: absolute;
  top:15px;
  right: 0px;

}

.show-area{
  background: #efeff4;
  width: 100%;
  height: auto;
}
.show-area .area-box{
  display: inline-block;
  border-radius: 20px;
  background: white;
  padding: 5px 5px;
  margin: 5px;
}

.weui-uploader__img {
  display:block;
  width:100%;
  height:calc(100vw/3.5);

}