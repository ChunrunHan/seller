.list-box{
  border-bottom:1px dashed #d1d2d7;
  border-top:1px dashed #d1d2d7; 
  /* background: #f7f8fa; */
  /* height: 50px; */
  /* background: #ebecf1; */
}
.list-box .box-content{
  position: relative;
  height: 60px;
  margin-left: 30px;
  border-left: 6px solid #dddde5;
  padding:8px 10px 8px 20px;

}
.list-box .box-content::before{
  content: '';
  position: absolute;
  width: 10px;
  height: 10px;
  background: #ebecf1;
  border-radius: 50%;
  top:50%;
  margin-top:-10px;
  left:-12px;
  border: 4px solid #ebecf1; 
}
.list-box .box-text{
  font-size: 14px;
  word-wrap:break-word;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  line-height: 18px;
  color:#95969a;
  word-spacing: 25px;
}
.list-box .box-time{
  font-size: 12px;
  color:#9c9da1;
  font-weight: 900;
  line-height: 16px;
  position: absolute;
  bottom: 5px;
}
.list-box .box-quan{
  width: 10px;
  height: 10px;
  border-radius: 50%;
  position: absolute;
  top:50%;
  margin-top:-6px;
  left:-8px;
}

